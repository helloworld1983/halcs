# sdbfs cmake module
#
# The following import targets are created
#
# ::
#
#   sdbfs-static
#   sdbfs
#
# This module sets the following variables in your project::
#
#   sdbfs_FOUND - true if sdbfs found on the system
#   sdbfs_INCLUDE_DIR - the directory containing sdbfs headers
#   sdbfs_LIBRARY -
#   sdbfs_STATIC_LIBRARY

@PACKAGE_INIT@

if(NOT TARGET sdbfs AND NOT TARGET sdbfs-static)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")

    if (TARGET sdbfs)
        get_target_property(@PROJECT_NAME@_INCLUDE_DIR
            sdbfs
            INTERFACE_INCLUDE_DIRECTORIES
        )
    else ()
        get_target_property(@PROJECT_NAME@_INCLUDE_DIR
            sdbfs-static
            INTERFACE_INCLUDE_DIRECTORIES
        )
    endif()

    if (TARGET sdbfs)
        get_target_property(@PROJECT_NAME@_LIBRARY
            sdbfs LOCATION
        )
    endif()
    if (TARGET sdbfs-static)
        get_target_property(@PROJECT_NAME@_STATIC_LIBRARY
            sdbfs-static LOCATION
        )
    endif()
endif()
